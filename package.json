{
  "name": "wisebox-app",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prod": "echo \"npm run dev\"",
    "postinstall": "node ./node_modules/husky/lib/bin install",
    "predeploy": "npm run dev",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run dev"
    }
  }
}
